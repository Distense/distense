{
  "name": "distense-frontend",
  "version": "0.3.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "analyze": "source-map-explorer build/main.*",
    "build": "node scripts/build.js",
    "dev": "DEV=true node scripts/start.js",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "codecov": "codecov"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "^6.26.0",
    "distense-contracts": "^1.0.0",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.4",
    "meteor-random": "0.0.3",
    "object-assign": "4.1.1",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-error-overlay": "^1.0.10",
    "react-helmet": "^5.1.3",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-scroll": "^1.6.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.72.0",
    "styled-jsx": "^1.0.11",
    "truffle-contract": "^2.0.5",
    "web3": "^0.20.0",
    "web3-utils": "^1.0.0-beta.27"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "^4.1.2",
    "chai-jest-diff": "^1.0.2",
    "chalk": "1.1.3",
    "compression-webpack-plugin": "^1.1.6",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^22.1.4",
    "mocha": "^5.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier": "1.10.2",
    "react-dev-utils": "^3.1.0",
    "react-test-renderer": "^16.2.0",
    "react-web3": "^0.4.5",
    "sinon": "^4.3.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "^3.5.1",
    "webpack-auto-inject-version": "^1.0.0",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/enzymeSetup.js"
  }
}
